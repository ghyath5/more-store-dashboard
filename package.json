{
	"name": "moreforless",
	"version": "1.0.0",
	"description": "More for less dashboard",
	"author": "Ghyath",
	"scripts": {
		"dev": "nuxt",
		"build": "nuxt build",
		"start": "nuxt start",
		"generate": "nuxt generate",
		"fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix"
	},
	"config": {
		"nuxt": {
			"host": "0.0.0.0",
			"port": "3000"
		}
	},
	"dependencies": {
		"@nuxtjs/apollo": "^4.0.1-rc.1",
		"core-js": "^2.6.10",
		"dotenv": "^8.2.0",
		"filepond": "^4.18.0",
		"filepond-plugin-file-validate-type": "^1.2.5",
		"filepond-plugin-image-preview": "^4.6.4",
		"graphql-tag": "^2.10.3",
		"js-cookie": "^2.2.1",
		"js2excel": "^1.0.1",
		"lodash": "^4.17.15",
		"nuxt": "^2.11.0",
		"vue-core-image-upload": "^2.4.12",
		"vue-filepond": "^6.0.2",
		"vuejs-paginate": "^2.1.0",
		"vuetify": "^2.3.3"
	},
	"devDependencies": {
		"@babel/core": "^7.10.2",
		"@babel/preset-env": "^7.10.2",
		"@mdi/font": "^4.9.95",
		"@nuxtjs/moment": "^1.6.1",
		"@nuxtjs/vuetify": "^1.11.2",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.0.3",
		"eslint": "^6.8.0",
		"eslint-config-standard": "^14.1.0",
		"eslint-config-vuetify": "^0.5.0",
		"eslint-loader": "^3.0.3",
		"eslint-plugin-html": "^6.0.0",
		"eslint-plugin-import": "^2.20.1",
		"eslint-plugin-node": "^11.0.0",
		"eslint-plugin-prettier": "^3.1.2",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-standard": "^4.0.1",
		"eslint-plugin-vue": "^6.2.1",
		"less": "^3.11.1",
		"less-loader": "^5.0.0",
		"sass": "^1.25.0",
		"sass-loader": "^8.0.2",
		"stylus": "^0.54.7",
		"stylus-loader": "^3.0.2"
	},
	"prettier": {
		"useTabs": true,
		"printWidth": 120,
		"tabWidth": 4,
		"trailingComma": "es5",
		"jsxBracketSameLine": false,
		"bracketSpacing": true,
		"semi": true,
		"singleQuote": true,
		"htmlWhitespaceSensitivity": "ignore"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true,
			"browser": true,
			"es6": true
		},
		"extends": [
			"plugin:vue/essential",
			"@vue/prettier"
		],
		"rules": {
			"no-console": "off",
			"vue/multiline-html-element-content-newline": [
				2,
				{
					"ignoreWhenEmpty": true,
					"ignores": [
						"pre",
						"textarea"
					],
					"allowEmptyLines": false
				}
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	}
}
